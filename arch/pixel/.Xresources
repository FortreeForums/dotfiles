! main colors {{
*background: #131209
*foreground: #CEB793
! Black + DarkGrey
*color0:  #060108
*color8:  #6A4D3B
! DarkRed + Red
*color1:  #4F3227
*color9:  #4F3227
! DarkGreen + Green
*color2:  #693C2C
*color10: #693C2C
! DarkYellow + Yellow
*color3:  #723A29
*color11: #723A29
! DarkBlue + Blue
*color4:  #6A4D3B
*color12: #6A4D3B
! DarkMagenta + Magenta
*color5:  #8C3B26
*color13: #8C3B26
! DarkCyan + Cyan
*color6:  #A95934
*color14: #A95934
! LightGrey + White
*color7:  #CEB793
*color15: #CEB793
! }}

! URxvt {{
URxvt*font: xft:MonteCarlo:size=12
URxvt*boldFont: xft:MonteCarlo:size=12
URxvt.italicFont: xft:MonteCarlo:size=12
URxvt.boldItalicFont: xft:MonteCarlo:size=12
URxvt.geometry: 80x30
URxvt.saveLines: 10000
URxvt.cutchars: "()*,<>[]{}|''"
URxvt.perl-ext-common: default,matcher,searchable-scrollback,selection-to-clipboard
URxvt.selection-autotransform.0: s/^([^:[:space:]]+):(\\d+):?$/+$2 \\Q$1\\E/
URxvt.skipBuiltinGlyphs: false
URxvt.url-launcher: /usr/bin/xdg-open
! URxvt.internalBorder: 10 - 10px padding
URxvt*colorUL: #A95934
URxvt.scrollBar: false
URxvt.visualBell: true
URxvt.cursorBlink: true
URxvt.cursorUnderline: true
URxvt.cursorColor:#4F3227
URxvt*title: ricu~
URxvt*lineSpace: -1
URxvt.imLocale: en_IE.UTF-8
! }}

! Font settings {{
Xft.hintstyle:              hintfull
Xft.hinting:                1
Xft.antialias:              0
Xft.rgba:                   rgb
Xft.lcdfilter:              lcddefault
Xft.dpi: 101
! }}
