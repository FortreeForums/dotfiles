! URxvt ///
URxvt*font: xft:MonteCarlo:size=13
URxvt*boldFont: xft:MonteCarlo:size=13
URxvt.italicFont: xft:MonteCarlo:pixelsize=13
URxvt.boldItalicFont: xft:MonteCarlo:pixelsize=13
URxvt.geometry: 80x30
URxvt.saveLines: 10000
URxvt.cutchars: "()*,<>[]{}|''"
URxvt.perl-ext-common: default,matcher,searchable-scrollback,selection-to-clipboard
URxvt.selection-autotransform.0: s/^([^:[:space:]]+):(\\d+):?$/+$2 \\Q$1\\E/
URxvt.skipBuiltinGlyphs: true
URxvt.url-launcher: C:/Program Files (x86)/Mozilla Firefox/firefox.exe
! URxvt.internalBorder: 10 - 10px padding
URxvt*colorUL: #522770
URxvt.scrollBar: false
URxvt.visualBell: true
URxvt.cursorBlink: true
URxvt.cursorUnderline: true
! URxvt.cursorColor:#0b0e17
URxvt*title: term
URxvt*lineSpace: -1
! \\\

! Font settings
Xft.hintstyle:              hintfull
Xft.hinting:                1
Xft.antialias:              false
Xft.rgba:                   rgb
Xft.lcdfilter:              lcdlight
Xft.dpi: 96
